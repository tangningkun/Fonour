@using System.Configuration
@{
    //SetAntiForgeryCookie();
    var version = ConfigurationManager.AppSettings["version"];
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <link rel="shortcut icon" href="~/Assets/images/ico/favicon.png">
    <add name="Access-Control-Allow-Headers" value="Origin,X-Requested-With,Content-Type,accept,key" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- CSS -->
    <link href="~/Assets/css/font.css" rel="stylesheet" />
    <link href="~/Assets/css/admin.css" rel="stylesheet" />
    <title>@ViewBag.Title - TSBLOG</title>
    <script type="text/javascript">
        //This is used to get the application's root path from javascript. It's useful if you're running application in a virtual directory under IIS.
        window.webapi = '@(System.Configuration.ConfigurationManager.AppSettings["webapi"])';
        var islogin = true;
        var _version = @version;
        var _jsPath = '@(ViewBag.JsPath ?? string.Empty)';
    </script>
</head>
<body>
    <!-- 顶部开始 -->
    <div class="container">
        <div class="logo"><a href="javascript:;"><h3>TsBlog</h3></a></div>
        <div class="left_open">
            <i title="展开左侧栏" class="iconfont">&#xe699;</i>
        </div>
        @Html.Partial("_NavBarLeft")
        @Html.Partial("_NavBarRight")

        
        
    </div>
    <!-- 顶部结束 -->
    <!-- 中部开始 -->
    <!-- 左侧菜单开始 -->
    <div class="left-nav">
        <div id="side-nav">
            <ul id="nav">
                <li>
                    <a href="javascript:;">
                        <i class="iconfont">&#xe6b8;</i>
                        <cite>会员管理</cite>
                        <i class="iconfont nav_right">&#xe697;</i>
                    </a>
                    <ul class="sub-menu">
                        <li date-refresh="1">
                            <a href="javascript:;">
                                <i class="iconfont">&#xe6a7;</i>
                                <cite>会员列表(静态表格)</cite>

                            </a>
                        </li>
                        <li>
                            <a href="javascript:;">
                                <i class="iconfont">&#xe6a7;</i>
                                <cite>会员列表(动态表格)</cite>

                            </a>
                        </li>
                        <li date-refresh="1">
                            <a href="javascript:;">
                                <i class="iconfont">&#xe6a7;</i>
                                <cite>会员删除</cite>

                            </a>
                        </li>
                        <li>
                            <a href="javascript:;">
                                <i class="iconfont">&#xe70b;</i>
                                <cite>会员管理</cite>
                                <i class="iconfont nav_right">&#xe697;</i>
                            </a>
                            <ul class="sub-menu">
                                <li>
                                    <a href="javascript:;">
                                        <i class="iconfont">&#xe6a7;</i>
                                        <cite>会员列表</cite>

                                    </a>
                                </li>
                                <li>
                                    <a href="javascript:;">
                                        <i class="iconfont">&#xe6a7;</i>
                                        <cite>会员删除</cite>

                                    </a>
                                </li>
                                <li>
                                    <a href="javascript:;">
                                        <i class="iconfont">&#xe6a7;</i>
                                        <cite>等级管理</cite>

                                    </a>
                                </li>

                            </ul>
                        </li>
                    </ul>
                </li>
            </ul>
        </div>
    </div>
    <!-- <div class="x-slide_left"></div> -->
    <!-- 左侧菜单结束 -->
    <!-- 右侧主体开始 -->
    <div class="page-content">
        <div class="layui-tab tab" lay-filter="xbs_tab" lay-allowclose="false">
            <ul class="layui-tab-title">
                <li class="home"><i class="layui-icon">&#xe68e;</i>我的桌面</li>
            </ul>
            <div class="layui-unselect layui-form-select layui-form-selected" id="tab_right">
                <dl>
                    <dd data-type="this">关闭当前</dd>
                    <dd data-type="other">关闭其它</dd>
                    <dd data-type="all">关闭全部</dd>
                </dl>
            </div>
            <div class="layui-tab-content">
                <div class="layui-tab-item layui-show">
                    @RenderBody();
                    @*<iframe src='./welcome.html' frameborder="0" scrolling="yes" class="x-iframe"></iframe>*@
                </div>
            </div>
            <div id="tab_show"></div>
        </div>
    </div>
    <div class="page-content-bg"></div>
    <!-- 右侧主体结束 -->
    <!-- 底部开始 -->
    <div class="footer">
        <div class="copyright">Copyright ©2019 TSBLOG TANG NING KUN</div>
    </div>
    <!-- 底部结束 -->
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Assets/layui/layui.js"></script>
    <script src="~/Assets/js/cookie.js"></script>
    <script src="~/Assets/js/admin.js"></script>
    <script src="~/Assets/lib/require.js" type="text/javascript"></script>
    <script src="~/Assets/lib/config.js" type="text/javascript"></script>
    @RenderSection("scripts", required: false)
</body>
</html>